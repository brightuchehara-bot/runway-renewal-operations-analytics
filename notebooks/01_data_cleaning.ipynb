import pandas as pd

equipment = pd.read_csv("../data/raw/equipment_logs.csv")
production = pd.read_csv("../data/raw/production_logs.csv")

equipment.dropna(inplace=True)
production['work_date'] = pd.to_datetime(production['work_date'])

equipment.to_csv("../data/processed/equipment_clean.csv", index=False)
production.to_csv("../data/processed/production_clean.csv", index=False)
